<!-- 
	Key
	[] 			- Required params
	key:type
	||			- Optional param list, favors left to right
	~ 			- Optional param
-->
<Window>
	<AppName value="Tactics" />					<!-- [value:string] 				The name in the upper left of the window -->
	<Resolution width="1920" height="1080" /> 	<!-- [height:int width:int] 		Height and width are required -->
</Window>

<Renderer>
	<ResolutionScale value=".9" />
</Renderer>
